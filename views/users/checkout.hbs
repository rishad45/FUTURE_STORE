        {{!-- jquery validator --}}

        <script>
            function removeAddress(addressId) {
                $.ajax({
                    url: '/removeAddress',
                    data: {
                        addressId: addressId
                    },
                    method: 'post',
                    success: function () {
                        Swal.fire({
                            title: 'Are you sure?',
                            text: "You won't be able to revert this!",
                            icon: 'warning',
                            showCancelButton: true,
                            confirmButtonColor: '#3085d6',
                            cancelButtonColor: '#d33',
                            confirmButtonText: 'Yes, delete it!'
                        }).then((result) => {
                            if (result.isConfirmed) {
                                Swal.fire(
                                    'Deleted!',
                                    'Your file has been deleted.',
                                    'success',
                                )
                            }
                        })

                    }
                })
                $("#addressDiv").load(location.href + " #addressDiv")


            };



            $("#checkoutForm").submit((e) => {
                e.preventDefault()
                $.ajax({
                    url: '/checkout',
                    method: 'post',
                    data: $("#checkoutForm").serialize(),
                    success: (res) => {
                        if (res.codSuccess) {
                            Swal.fire("ORDER CONFIRMED") 
                            location.href = '/myOrders' 
                        } else {
                            console.log("1")
                            console.log(res)
                            razorPayPayment(res);
                        }
                    }
                })
            });

            $("#addressForm").submit((e) => {
                e.preventDefault(); 
                $.ajax({
                    url: '/saveAddress',
                    data: $("#addressForm").serialize(),
                    method: 'post',
                    success: (res) => {
                        location.reload()
                    }
                })
            })

            function razorPayPayment(order) {
                let options = {
                    "key": "rzp_test_jwDTlUJaAFAea0", // Enter the Key ID generated from the Dashboard
                    "amount": order.amount, // Amount is in currency subunits. Default currency is INR. Hence, 50000 refers to 50000 paise
                    "currency": "INR",
                    "name": "Future Store",
                    "description": "Test Transaction",
                    "image": "https://example.com/your_logo",
                    "order_id": order.id, //This is a sample Order ID. Pass the `id` obtained in the response of Step 1
                    "callback_url": "https://eneqd3r9zrjok.x.pipedream.net/",

                    "handler": function (res) {
                        $.ajax({
                            url: '/verifyPayment',
                            method: 'post',
                            data: {
                                res,
                                order
                            },
                            success: (response) => {
                                if (response.status) {
                                    console.log("response is", response)
                                    console.log("response.status  is", response.status)
                                    location.href = '/myOrders'
                                } else {
                                    console.log("payment failed response", response.status)
                                }
                            }
                        })
                    },
                    "prefill": {
                        "name": "Muhammed Rishad",
                        "email": "45rishadricu@gmail.com",
                        "contact": "7559979974"
                    },
                    "notes": {
                        "address": "Razorpay Corporate Office"
                    },
                    "theme": {
                        "color": "#3399cc"
                    }
                };

                var rzp1 = new Razorpay(options);
                rzp1.open();
            };
        </script>
