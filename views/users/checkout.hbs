    <!-- ...:::: Start Breadcrumb Section:::... -->
    <div class="breadcrumb-section breadcrumb-bg-color--golden">
        <div class="breadcrumb-wrapper">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <h3 class="breadcrumb-title">Checkout</h3>
                        <div class="breadcrumb-nav breadcrumb-nav-color--black breadcrumb-nav-hover-color--golden">
                            <nav aria-label="breadcrumb">
                                <ul>
                                    <li><a href="index.html">Home</a></li>
                                    <li><a href="shop-grid-sidebar-left.html">Shop</a></li>
                                    <li class="active" aria-current="page">Checkout</li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- ...:::: End Breadcrumb Section:::... -->

    <!-- ...:::: Start Checkout Section:::... -->
    <div class="checkout-section">
        <form id="checkoutForm">
            <div class="container " id="addressDiv">
                <div class="row">
                    <!-- User Quick Action Form -->
                    <div class="col-8">
                        {{!-- add new address --}}
                        <!-- Button trigger modal -->
                        <button type="button" class="btn btn-golden mb-3" data-bs-toggle="modal"
                            data-bs-target="#staticBackdrop">
                            <i class="fa fa-plus"></i> ADD NEW ADDRESS
                        </button>

                        <!-- Modal -->
                        {{!-- default Items --}}
                        <input type="text" name="userId" id="" value="{{user._id}}" hidden>
                        {{!-- <input type="text" name=""> --}}


                        {{!-- addresses --}}
                        {{#if isLength}}
                        {{#each addresses.address}}
                        <div class="gridPay">
                            <label class="cardPay">
                                <input name="address" class="radioPay" type="radio" value="{{this._id}}" checked>
                                <span class="plan-details">
                                    <span class="plan-type">{{this.fullname}}</span>
                                    <span>{{this.mobile}}</span>
                                    <span>{{this.address}}</span>
                                    <span>{{this.pincode}}</span>
                                    <a onclick="removeAddress('{{this._id}}')" class="ml-auto"><i
                                            class="fa fa-trash"></i></a>
                                    {{!-- <a href="/removeAddress/{{this._id}}" class="ml-auto btn btn-danger"><i
                                            class="fa fa-trash"></i></a> --}}


                                </span>
                            </label>
                        </div>
                        {{/each}}
                        {{else}}
                        <div class="empty-state">
                            <div class="empty-state__content">
                                <div class="empty-state__icon">
                                    <img src="data:image/png;base64,"
                                        alt="">
                                </div>
                                <div class="empty-state__message">No address has been added yet.</div>
                                <div class="empty-state__help">
                                    Add a new address simply by clicking on the button above
                                </div>
                            </div>
                        </div>
                        {{/if}}
                        <!-- User Quick Action Form -->
                    </div>
                    <div class="col-4">

                        <div class="coupon_code right cartTotal" data-aos="fade-up" data-aos-delay="400"
                            style="position: sticky;top:30vh;bottom:0;z-index: 2;">
                            <h3>Cart Totals</h3>
                            <div class="coupon_inner">
                                <div class="cart_subtotal">
                                    <p>Price ({{cartLength}} items)</p>
                                    <p class="cart_amount">${{cartTotal}}</p>
                                </div>
                                <div class="cart_subtotal ">
                                    <p>Shipping</p>
                                    <p class="cart_amount">$0.00</p>
                                    {{!-- <p class="cart_amount"><span>Flat Rate:</span> $255.00</p> --}}


                                </div>
                                <div class="cart_subtotal ">
                                    <p>Discounts</p>
                                    <p class="cart_amount">{{discountAmount}}</p>
                                </div>
                                <div class="cart_subtotal ">
                                    <p>Meta Coins Discount</p>
                                    <p class="cart_amount">$0.00</p>
                                </div>
                                {{!-- <a href="#">Calculate shipping</a> --}}

                                <div class="cart_subtotal">
                                    <p>Total</p>
                                    <p class="cart_amount">${{totalPayable}}</p> 
                                </div>
                                <p class="text-success">You will save ${{discountAmount}} on this order</p>
                                <p><i class="fa fa-shield mr-2"></i>secure payment</p> 

                            </div>
                        </div>
                    </div>
                    <!-- Start User Details Checkout Form -->
                    {{#if isLength}}
                    <div class="container container1">
                        <div class="plans">
                            <div class="title">Choose a Payment method</div>
                            <label class="plan basic-plan" for="basic">
                                <input checked type="radio" name="paymentMethod" id="basic" value="COD" />
                                <div class="plan-content">
                                    <img loading="lazy" src="/user/images/icons/cod.jpg" alt="" />
                                    <div class="plan-details">
                                        <span>Cash on delivery</span>
                                        <p>For smaller business, with simple salaries and pay schedules.</p>
                                    </div>
                                </div>
                            </label>

                            <label class="plan complete-plan" for="complete">
                                <input type="radio" id="complete" name="paymentMethod" value="RAZOR" />
                                <div class="plan-content">
                                    <img loading="lazy" src="/user/images/icons/razor.png" alt="" />
                                    <div class="plan-details">
                                        <span>Online Payment</span>
                                        <p>For growing business who wants to create a rewarding place to work.</p>
                                    </div>
                                </div>

                            </label>

                            <label class="plan complete-plan" for="paypal">
                                <input type="radio" id="paypal" name="paymentMethod" value="PAYPAL" />
                                <div class="plan-content">
                                    <img loading="lazy" src="/user/images/icons/online.png" alt="" />
                                    <div class="plan-details">
                                        <span>PayPal Account</span>
                                        <p>PayPal is the faster and most trusted way to send and receive money</p>
                                    </div>
                                </div>

                            </label>
                            <div class="mt-5">
                                <button class="btn btn-md btn-success" type="submit" id="pay-button">PLACE
                                    ORDER</button>
                            </div>
                        </div>
                    </div>
                    {{/if}}

                </div>





            </div><!-- ...:::: End Checkout Section:::... -->
        </form>
        <div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
            aria-labelledby="staticBackdropLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="staticBackdropLabel">CREATE NEW ADDRESS</h5>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <form action="/saveAddress" id="addressForm">
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="default-form-box">
                                        <label>Full Name <span>*</span></label>
                                        <input type="text" name="fullName">
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="default-form-box">
                                        <label>10 digit mobile number <span>*</span></label>
                                        <input type="text" name="mobile">
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="default-form-box">
                                        <label>Pincode</label>
                                        <input type="text" name="pincode">
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="default-form-box">
                                        <label>Locality</label>
                                        <input type="text" name="locality">
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="default-form-box">
                                        <label>Address</label>
                                        <textarea name="address" id="" cols="30" rows="10"></textarea>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="select_form_select default-form-box">
                                        <label for="stat_name">country <span>*</span></label>
                                        <select class="niceselect_option wide" name="state" id="state_name">
                                            <option value="kerala">kerala</option>
                                            <option value="punjab">punjab</option>
                                            <option value="haryana">haryana</option>
                                        </select>
                                    </div>

                                </div>
                                <div class="col-12">
                                    <div class="default-form-box">
                                        <label>City/district/Town<span>*</span></label>
                                        <input placeholder="City/district/Town" type="text" name="city">
                                    </div>
                                </div>

                                <div class="col-12">
                                    <div class="default-form-box">
                                        <label>Landmark<span>*</span></label>
                                        <input type="text" name="landmark">
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="default-form-box">
                                        <label>Alternate Phone<span>*</span></label>
                                        <input type="text" name="altPhone">
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="default-form-box">
                                        <label> Email Address <span>*</span></label>
                                        <input type="text" name="email">
                                    </div>
                                </div>
                                <div class="col-lg-12">
                                    <input type="radio" name="addType" id="home" value="home">
                                    <label for="home">HOME</label>
                                    <input type="radio" name="addType" id="office" value="office">
                                    <label for="office">OFFICE</label>

                                </div>


                            </div>

                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                            <button type="submit" class="btn btn-primary">SAVE ADDRESS</button>
                        </div>

                    </form>
                </div>
            </div>
        </div>




        <!-- Start Footer Section -->
        {{>userFooter}}
        <!-- End Footer Section -->

