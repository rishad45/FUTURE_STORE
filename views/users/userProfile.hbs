                                                <p class="p">Valid till {{formatString couponDetails.expiryDate}}</p>
                                                <p>Minimum spend :<span class="text-danger">
                                                        ${{couponDetails.minimumSpend}}</span></p>


                                            </div>
                                        </div>
                                        {{#if coupons.redeemed}} 
                                        <div class="copy-button">
                                            
                                            {{!-- <input id="copyvalue" type="text" readonly
                                                value="" disabled/> --}}
                                            <button class="copybtn" disabled >REDEEMED</button>
                                        </div>
                                        {{else}}
                                        <div class="copy-button">
                                            <input id="copyvalue" type="text" readonly
                                                value="{{couponDetails.couponCode}}" />
                                            <button onclick="copyIt()" class="copybtn">COPY</button>
                                        </div>
                                        
                                        {{/if}} 
                                    </div>
                                </div>
                                {{/each}}
                            </div>





