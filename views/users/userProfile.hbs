                        <div class="tab-pane fade" id="orders">
                            <h4 class="ml-2"> MANAGE ADDRESSES</h4>
                            <div class="container" id="addressDiv">
                                <div class="row">
                                    <div class="col-12 mt-3 mb-2">
                                        <button type="button" class="btn btn-secondary btn-lg btn-block"
                                            data-bs-toggle="modal" data-bs-target="#staticBackdrop"><span
                                                class="fa fa-plus"></span> Add new Address</button>
                                        </button>
                                    </div>
                                    {{#each address}}
                                    <div class="col-4">
                                        <div class="card" style="width: 18rem;">
                                            <div class="card-body">
                                                <h5 class="card-title">{{this.fullname}}</h5>
                                                <h6 class="card-subtitle mb-2 text-muted">{{this.mobile}}</h6>
                                                <p class="card-text">{{this.locality}} <br> {{this.city}},
                                                    {{this.state}}<br> {{this.pincode}}</p>
                                                <button class="card-link text-success"
                                                    onclick="updateAddress('{{this._id}}') "> edit </button>
                                                <button class="card-link text-danger"
                                                    onclick="deleteAddress('{{this._id}}') "> delete </button>
                                            </div>
                                        </div>
                                    </div>
                                    {{/each}}
                                </div>
                            </div>
                        </div>
                        <div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false"
                            tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                            <div class="modal-dialog">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title" id="staticBackdropLabel">CREATE NEW ADDRESS</h5>
                                        <button type="button" class="btn-close" data-bs-dismiss="modal"
                                            aria-label="Close"></button>
                                    </div>
                                    <form id="createAddress">
                                        <div class="modal-body">
                                            <div class="row">
                                                <div class="col-lg-6">
                                                    <div class="default-form-box">
                                                        <label>Full Name <span>*</span></label>
                                                        <input type="text" name="fullName" id="fullName">
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="default-form-box">
                                                        <label>10 digit mobile number <span>*</span></label>
                                                        <input type="text" name="mobile">
                                                    </div>
                                                </div>
                                                <div class="col-12">
                                                    <div class="default-form-box">
                                                        <label>Pincode</label>
                                                        <input type="text" name="pincode">
                                                    </div>
                                                </div>
                                                <div class="col-12">
                                                    <div class="default-form-box">
                                                        <label>Locality</label>
                                                        <input type="text" name="locality">
                                                    </div>
                                                </div>
                                                <div class="col-12">
                                                    <div class="default-form-box">
                                                        <label>Address</label>
                                                        <textarea name="address" id="" cols="30" rows="10"></textarea>
                                                    </div>
                                                </div>
                                                <div class="col-12">
                                                    <div class="select_form_select default-form-box">
                                                        <label for="stat_name">country <span>*</span></label>
                                                        <select class="niceselect_option wide" name="state"
                                                            id="state_name">
                                                            <option value="kerala">kerala</option>
                                                            <option value="punjab">punjab</option>
                                                            <option value="haryana">haryana</option>
                                                        </select>
                                                    </div>

                                                </div>
                                                <div class="col-12">
                                                    <div class="default-form-box">
                                                        <label>City/district/Town<span>*</span></label>
                                                        <input placeholder="City/district/Town" type="text" name="city">
                                                    </div>
                                                </div>

                                                <div class="col-12">
                                                    <div class="default-form-box">
                                                        <label>Landmark<span>*</span></label>
                                                        <input type="text" name="landmark">
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="default-form-box">
                                                        <label>Alternate Phone<span>*</span></label>
                                                        <input type="text" name="altPhone">
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="default-form-box">
                                                        <label> Email Address <span>*</span></label>
                                                        <input type="text" name="email">
                                                    </div>
                                                </div>
                                                <div class="col-lg-12">
                                                    <input type="radio" name="addType" id="home" value="home">
                                                    <label for="home">HOME</label>
                                                    <input type="radio" name="addType" id="office" value="office">
                                                    <label for="office">OFFICE</label>

                                                </div>


                                            </div>

                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-secondary"
                                                data-bs-dismiss="modal">Close</button>
                                            <button type="submit" class="btn btn-primary" id="save">SAVE
                                                ADDRESS</button>
                                        </div>

                                    </form>


                                </div>
                            </div>
                        </div>


